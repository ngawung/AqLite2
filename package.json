{
  "name": "CeteraLauncher",
  "version": "0.0.1",
  "description": "launcher for cetera bot",
  "main": "main.js",
  "scripts": {
    "dump": "./cetera/dump_projector ./cetera/'Cetera R10.3 Flash Projector.exe' ./cetera/cetera.swf",
    "start": "electron .",
    "pack": "electron-builder --dir",
    "dist": "electron-builder"
  },
  "keywords": [
    "aqw;",
    "aqlite2;"
  ],
  "author": "ngawung",
  "license": "MIT",
  "devDependencies": {
    "electron": "^4.2.12",
    "electron-builder": "^22.8.1",
    "electron-localshortcut": "^3.2.1",
    "nw-flash-trust": "^0.3.0"
  },
  "build": {
    "asar": false,
    "appId": "ngawung",
    "mac": {
      "category": "Games"
    },
    "linux": {
      "target": [
        {
          "target": "AppImage",
          "arch": [
            "x64"
          ]
        }
      ],
      "category": "Game"
    },
    "win": {
      "target": [
        {
          "target": "nsis",
          "arch": [
            "x64"
          ]
        }
      ]
    },
    "icon": "Icon/Icon.png"
  },
  "dependencies": {
    "electron-localshortcut": "^3.2.1",
    "nw-flash-trust": "^0.3.0"
  }
}
